const express = require('express');
const router = express.Router();
const User = require('../model/user');

router.get('/', (req, res) => {
    res.render('index');
})
router.get('/login', (req, res) => {
    res.render('login');
})
router.get('/forgot', (req, res) => {
    res.render('forgot');
})

module.exports = router;